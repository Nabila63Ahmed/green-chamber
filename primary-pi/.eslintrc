{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "function-paren-newline": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-use-before-define": 0,
    "object-curly-newline": 0,
    "import/prefer-default-export": 0,
    "prefer-destructuring": 0,
    "operator-linebreak": 0
  }
}
